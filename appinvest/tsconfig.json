{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/admin/Admins.jsx",
    "src/components/admin/Commissions.jsx",
    "src/components/admin/Content.jsx",
    "src/components/admin/Dashboard.jsx",
    "src/components/admin/Deposits.jsx",
    "src/components/admin/Microtasks.jsx",
    "src/components/admin/Plans.jsx",
    "src/components/admin/Points.jsx",
    "src/components/admin/Referrals.jsx",
    "src/components/admin/Security.jsx",
    "src/components/admin/Settings.jsx",
    "src/components/admin/Transactions.jsx",
    "src/components/admin/Users.jsx",
    "src/components/admin/Withdrawals.jsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
